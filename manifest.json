{
    "manifest_version": 3,
    "name": "Mon extension",
    "version": "1.0.0",
    "description": "Description de l'extension",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "https://api.openai.com/"
    ],
    "options_page": "options/options.html",
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["content-script.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Titre de l'extension"
    }
  }
  